# Get list of versions to be used by tests/check_for_new_version

set -euo pipefail
[ -n "${DEBUG:-}" ] && set -x

get_versions(){
    curl https://api.github.com/repos/actions/runner/releases -sS --fail |
    jq -r '.[].name'
}

get_versions
